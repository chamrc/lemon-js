  },
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/mocha": "^2.2.41",
    "@types/mongoose": "^4.7.11",
    "@types/node": "^7.0.18",
    "chai": "^3.5.0",
    "mocha": "^3.3.0",
    "ts-node": "^3.0.4",
    "tslint": "^5.2.0",
    "typescript": "^2.3.2"
